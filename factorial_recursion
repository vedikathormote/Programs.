public class Factorial {
    public static int calfact(int n){
        if(n==1||n==0){
            return 1;
        }
        int fact1=calfact(n-1);
        int fact2=n*fact1;
        return fact2;
    }

    public static void main(String[] args) {
        int n=5;
        int ans=calfact(5);
        System.out.println(ans);
    }
}
