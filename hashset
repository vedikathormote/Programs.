
import java.util.HashSet;
public class Hashsett {
    public static void main(String[] args) {
        HashSet<Integer> set=new HashSet<>();
        set.add(10);
        set.add(20);
        set.add(30);
        if(set.contains(20));{
            System.out.println("yes present");
        }
        set.remove(30);
        System.out.println(set);
        set.add(20);
        set.add(40);
        System.out.println(set);
        System.out.println(set.size());
    }

}
